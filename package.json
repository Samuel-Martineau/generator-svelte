{
  "name": "generator-svelte",
  "version": "1.3.0",
  "description": "Yeoman generator that scaffolds Svelte.js apps",
  "homepage": "https://github.com/Samuel-Martineau/generator-svelte",
  "author": {
    "name": "Samuel-Martineau",
    "email": "samumartineau@gmail.com",
    "url": "https://smartineau.me"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "svelte",
    "svelte.js",
    "generator",
    "yo",
    "yeoman",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^4.0.7",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "generator-license": "^5.4.0",
    "latest-version": "^5.1.0",
    "yeoman-generator": "^4.4.0",
    "yosay": "^2.0.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "Samuel-Martineau/generator-svelte",
  "license": "Apache-2.0"
}
